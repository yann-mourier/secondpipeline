pipeline {
  agent any
  
  stages {
 
    stage('tmp') {
      steps {
        echo "etape de test"
      }
    }
    
    stage('build') {
      environment {
       AWS_SECRET = credentials('monmotdepassesecret')
       }
      steps {
        echo "etape de build"
        sh 'printenv'
      }
    }
  }
}