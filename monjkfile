pipeline {
  agent any
  
  stages {
 
    stage('build') {
      steps {
        echo "etape de test"
        sh 'tree /tmp'
      }
    }
    stage('build en LANG=C') {
      environment {
        LANG = 'C'
      }
      steps {
        echo "etape de test avec environnement"
        sh 'tree /tmp'
      }
    }
  }
}