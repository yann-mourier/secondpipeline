pipeline {
  agent any
  
  stages {
 
    stage('test') {
      steps {
        echo "etape de test"
        sleep 10
      }
    }

    stage('deploiement') {
    parallel { 
         stage('tache parallele une') {
           steps {
             echo "message parrallele un"
             sleep 10
           }
         }
         stage('tache parrallele deux') {
           steps {
              echo "message parralele deux"
           }
         }
     }
    }
    }
}